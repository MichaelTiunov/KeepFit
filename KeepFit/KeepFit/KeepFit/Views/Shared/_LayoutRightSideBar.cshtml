<!DOCTYPE HTML>
<html>
<head>
    <title>Right Sidebar - Verti by HTML5 UP</title>
    <meta http-equiv="content-type" content="text/html; charset=utf-8" />
    <meta name="description" content="" />
    <meta name="keywords" content="" />
    <link href="http://fonts.googleapis.com/css?family=Open+Sans:300,800" rel="stylesheet" type="text/css" />
    <link href="http://fonts.googleapis.com/css?family=Oleo+Script:400" rel="stylesheet" type="text/css" />
    <link rel="stylesheet" href="http://code.jquery.com/ui/1.10.3/themes/smoothness/jquery-ui.css">
    @RenderSection("styles", false)
    <script src="~/Assets/js/jquery.min.js"></script>
    <script src="http://code.jquery.com/ui/1.10.3/jquery-ui.js"></script>
    <script src="~/Assets/js/config.js"></script>
    <script src="~/Assets/js/skel.min.js"></script>
    <script src="~/Assets/js/skel-panels.min.js"></script>
    <script src="~/Assets/js/jquery.loadTemplate-1.3.2.js"></script>
    <link href="~/Assets/css/style.css" rel="stylesheet" />
    <link href="~/Assets/css/style-desktop.css" rel="stylesheet" />
    <link href="~/Assets/css/main.css" rel="stylesheet" />
</head>
<body class="right-sidebar">
    @RenderBody()
    <!-- Header Wrapper -->
    @Html.Partial("MainMenu")

    <!-- Main Wrapper -->
    <div id="main-wrapper">
        <div class="container">
            <div class="row">
                <div class="8u skel-cell-important">
                    <!-- Content -->
                    <div id="content">
                        <article class="last">
                            @RenderSection("RightContent", false)
                        </article>
                    </div>

                </div>
                <div class="4u">
                    <!-- Sidebar -->
                    <div id="sidebar">
                        @RenderSection("RightSidebar", false)
                    </div>

                </div>
            </div>
        </div>
    </div>
    <!-- Footer Wrapper -->
    @Html.Partial("Footer")
    @RenderSection("scripts", false)
</body>
</html>
